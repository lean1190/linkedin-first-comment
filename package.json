{
  "name": "linkedin-first-comment",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=22.11.0 <=23.7.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "test": "vitest run --coverage --coverage.all=false",
    "events:dev": "npx inngest-cli@latest dev",
    "supabase:types": "source .env.local && supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/lib/supabase/types.ts",
    "lint:all": "npx @biomejs/biome check --write .",
    "lint:staged": "npx @biomejs/biome check --staged --write .",
    "types:check": "npx tsc --skipLibCheck --noEmit",
    "supabase:login": "npx supabase login",
    "dependencies:check": "ncu",
    "dependencies:update": "ncu -u && npm install && npm run lint:all",
    "dependencies:interactive": "ncu --interactive",
    "upgrade:next": "npx @next/codemod",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky"
  },
  "dependencies": {
    "@date-fns/utc": "2.1.0",
    "@hookform/resolvers": "^4.0.0",
    "@inngest/middleware-encryption": "1.0.0",
    "@next-safe-action/adapter-react-hook-form": "^1.0.13",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "1.2.3",
    "@radix-ui/react-slot": "1.1.2",
    "@supabase/ssr": "0.5.2",
    "@supabase/supabase-js": "2.48.1",
    "@tabler/icons-react": "3.30.0",
    "@tsparticles/engine": "3.8.1",
    "@tsparticles/react": "3.0.0",
    "@tsparticles/slim": "3.8.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "framer-motion": "12.4.2",
    "inngest": "3.31.7",
    "ky": "1.7.5",
    "next": "15.1.7",
    "next-safe-action": "^7.10.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "3.0.1",
    "use-debounce": "^10.0.4",
    "uuid": "^11.0.5",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@next/bundle-analyzer": "15.1.7",
    "@tailwindcss/postcss": "^4.0.6",
    "@types/node": "22",
    "@types/react": "19",
    "@types/react-dom": "19",
    "@vitest/coverage-v8": "^3.0.5",
    "cross-env": "7.0.3",
    "husky": "9.1.7",
    "lint-staged": "^15.4.3",
    "npm-check-updates": "17.1.14",
    "postcss": "8",
    "supabase": "2.12.1",
    "tailwindcss": "^4.0.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5",
    "vitest": "^3.0.5"
  }
}
