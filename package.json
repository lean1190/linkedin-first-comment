{
  "name": "linkedin-first-comment",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=22.11.0 <=24.5.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "test": "vitest run --coverage --coverage.all=false",
    "events:dev": "npx inngest-cli@latest dev",
    "supabase:types": "source .env.local && supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/lib/supabase/types.ts",
    "lint:all": "npx @biomejs/biome check --write .",
    "lint:staged": "npx @biomejs/biome check --staged --write .",
    "types:check": "npx tsc --skipLibCheck --noEmit",
    "supabase:login": "npx supabase login",
    "dependencies:check": "ncu",
    "dependencies:update": "ncu -u && npm install && npm run lint:all",
    "dependencies:interactive": "ncu --interactive",
    "upgrade:next": "npx @next/codemod",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky"
  },
  "dependencies": {
    "@date-fns/utc": "2.1.1",
    "@hookform/resolvers": "5.2.2",
    "@inngest/middleware-encryption": "1.0.2",
    "@next-safe-action/adapter-react-hook-form": "2.0.0",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-slot": "1.2.3",
    "@supabase/ssr": "0.7.0",
    "@supabase/supabase-js": "2.58.0",
    "@tabler/icons-react": "3.35.0",
    "@tsparticles/engine": "3.9.1",
    "@tsparticles/react": "3.0.0",
    "@tsparticles/slim": "3.9.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "framer-motion": "12.23.22",
    "inngest": "3.44.1",
    "ky": "1.11.0",
    "next": "15.5.4",
    "next-safe-action": "8.0.11",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.64.0",
    "tailwind-merge": "3.3.1",
    "use-debounce": "10.0.6",
    "uuid": "13.0.0",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@next/bundle-analyzer": "15.5.4",
    "@tailwindcss/postcss": "4.1.14",
    "@types/node": "24",
    "@types/react": "19",
    "@types/react-dom": "19",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "10.1.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.3",
    "npm-check-updates": "19.0.0",
    "postcss": "8.5.6",
    "supabase": "2.48.3",
    "tailwindcss": "4.1.14",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}
